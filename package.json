{
  "name": "governator-api-server",
  "version": "1.1.0",
  "description": "",
  "author": "Governator team",
  "private": false,
  "license": "MIT",
  "scripts": {
    "precommit": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && yarn test && yarn build",
    "prebuild": "rimraf dist",
    "build": "yarn prebuild && nest build",
    "codegen": "yarn test:generate && openapi-generator-cli generate -i ./governator-api-spec.json -g typescript-axios -o src/api-client --additional-properties=withoutPrefixEnums=true,withInterfaces=true,supportsES6=true,npmName=$API_CLIENT_NPM_PACKAGE_NAME,npmVersion=$npm_package_version",
    "codegen:dev": "export NODE_ENV=development eval $(grep '^NPM_TOKEN' ./.env.development) eval $(grep '^API_CLIENT_NPM_PACKAGE_NAME' ./.env.development) && yarn codegen && cd ./src/api-client && yarn publish --verbose --new-version $npm_package_version",
    "build:dev": "yarn build:client:dev && yarn prebuild && nest build",
    "build:prod": "yarn build:client:prod && yarn prebuild && nest build",
    "start": "nest start",
    "start:dev": "NODE_ENV=development yarn start --watch",
    "start:debug": "NODE_ENV=development yarn start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "export NODE_ENV=test && jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit",
    "test:generate": "jest --config ./test/jest-generate.json"
  },
  "dependencies": {
    "@nestjs/bull": "^0.6.2",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/common": "^9.4.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^9.4.1",
    "@nestjs/mongoose": "^9.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/throttler": "^4.0.0",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@snapshot-labs/snapshot.js": "^0.3.87",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link-http": "^1.5.17",
    "axios": "^1.1.3",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^7.1.0",
    "cron": "^2.3.0",
    "ethers": "^6.4.0",
    "eventsource": "^2.0.2",
    "express-session": "^1.17.3",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.1",
    "joi": "^17.6.0",
    "keccak": "^3.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.2.0",
    "node-fetch": "3.3.1",
    "openapi-types": "^12.1.1",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "passport-headerapikey": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.2.0",
    "siwe": "^2.0.3",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.1",
    "@types/express": "4.17.17",
    "@types/express-session": "^1.17.6",
    "@types/jest": "29.5.1",
    "@types/node": "^20.2.3",
    "@types/passport": "^1.0.12",
    "@types/passport-discord": "^0.1.6",
    "@types/passport-http": "^0.3.9",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "jest-openapi": "^0.14.2",
    "mongodb-memory-server": "^8.13.0",
    "rxjs-spy": "^8.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "typescript-eslint": "^0.0.1-alpha.0"
  },
  "engines": {
    "node": "18.13.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
